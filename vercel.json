{
  "version": 2,
  "builds": [
    {
      "src": "sgpdgs_project/sgpdgs/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sgpdgs_project/sgpdgs/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "sgpdgs.settings"
  }
}
